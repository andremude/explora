
<%#  INICIO DEL CARUSEL %>
<div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
  <ol class="carousel-indicators">
    <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
    <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
    <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
  </ol>
  <div class="carousel-inner d-flex align-items-center">
    <div class="carousel-item active">
      <img class="d-block w-100" src="https://www.peruhop.com/wp-content/uploads/cuantas-personas-visitan-machu-picchu-portada2-2.jpg" alt="First slide">
    </div>
    <div class="carousel-item">
      <img class="d-block w-100" src="https://www.confiep.org.pe/wp-content/uploads/2019/04/turismo1.jpg"  alt="Second slide">
    </div>
    <div class="carousel-item">
      <img class="d-block w-100" src="https://volawaytravel.pe/wp-content/uploads/2018/09/turismo-peru-puno.jpg" alt="Third slide">
    </div>
    <div class="container container-search ml-3 row ">
      <div class="overlay offset-md-3 col-xs-10 col-md-6  overlay-search ml-4 mr-4 pt-4 pb-5 pl-4 pr-4">
        <div class="row ">
          <div class="col">
            <p class="p-col-overlay ">Descubre increibles lugares, en todas partes</p>
          </div>
        </div>
        <div class="row">
          <div class="col-12">
            <div class="input-group mb-3">
              <div class="input-group-prepend">
                <span class="input-group-text" id="basic-addon1"><i class="fas fa-search-location" style="font-size:1.5rem;"></i></span>
              </div>
              <%= form_tag tours_path, method: :get do %>
                <div class="d-flex">
                  <%= text_field_tag :query,
                    params[:query],
                    class: "form-control py-4 search-text-size-input input-group py-3",
                    placeholder: "Encuentra tu tour"
                  %>
                  <%= submit_tag "Search", class: "btn btn-primary rounded py-3 px-5 ml-1 search-text-size-boton" %>
                </div>
              <% end %>
            </div>
          </div>
        </div>
      </div>

    </div>
  </div>

</div>
<%#  FIN DEL CARUSEL %>

